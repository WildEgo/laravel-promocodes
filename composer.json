{
  "name": "zgabievi/promocodes",
  "description": "Promotional Codes Generator for Laravel 5-9",
  "keywords": [
    "promo code",
    "coupon code",
    "discount code",
    "laravel",
    "php"
  ],
  "homepage": "https://github.com/zgabievi/promocodes",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Zura Gabievi",
      "email": "zura.gabievi@gmail.com"
    },
    {
      "name": "Oskars Germovs",
      "email": "oskars_germovs@inbox.lv"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "doctrine/dbal": "^2.0|^3.0",
    "laravel/framework": "^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4|^5|^6|^7|^8|^9|^10",
    "orchestra/testbench": "~4|~5|~6|~7|~8"
  },
  "autoload": {
    "psr-4": {
      "Gabievi\\Promocodes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gabievi\\Promocodes\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gabievi\\Promocodes\\PromocodesServiceProvider"
      ],
      "aliases": {
        "Promocodes": "Gabievi\\Promocodes\\Facades\\Promocodes"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
